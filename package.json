  "bugs": {
    "url": "https://github.com/timmolendijk/ts-transform-describe-component/issues"
  },
  "homepage": "https://github.com/timmolendijk/ts-transform-describe-component#readme",
  "devDependencies": {
    "@types/node": "^8.0.20",
    "tap": "^10.7.1",
    "ts-node": "^3.3.0",
