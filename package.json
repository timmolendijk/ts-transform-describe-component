    "url": "git+https://github.com/timmolendijk/ts-transform-describe-component.git"
  },
  "keywords": [
    "typescript",
    "compiler",
    "plugin",
    "transformer",
    "react",
    "component",
    "meta",
    "annotation",
    "description",
    "styleguide"
  ],
  "author": "Tim Molendijk <npm@timmolendijk.nl>",
